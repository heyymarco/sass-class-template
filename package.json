  "bugs": {
    "url": "https://github.com/heyymarco/sass-class-template/issues"
  },
  "homepage": "https://github.com/heyymarco/sass-class-template#readme",
  "dependencies": {
    "@heymarco/sass-selector": "^2.0.0",
    "@heymarco/sass-string": "^2.0.1"
  },
